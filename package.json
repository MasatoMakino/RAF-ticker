{
  "name": "@masatomakino/raf-ticker",
  "version": "0.2.10",
  "description": "Simple ticker with requestAnimationFrame.",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/RAF-ticker.git"
  },
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "eventemitter3": "^4.0.7"
  },
  "devDependencies": {
    "@masatomakino/gulptask-demo-page": "^0.4.4",
    "@masatomakino/gulptask-dev-server": "^0.4.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "prettier": "^2.5.0",
    "pretty-quick": "^3.1.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "prepare": "husky install",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run demo & npm run buildTS",
    "demo": "npx @masatomakino/gulptask-demo-page",
    "buildTS": "npx tsc --project tsconfig.cjs.json & tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "npx @masatomakino/gulptask-dev-server -b ./docs/demo",
    "watch:demo": "npm run demo -- -W",
    "watch:tsc": "tsc -W --incremental --project tsconfig.cjs.json & tsc -W --incremental --project tsconfig.esm.json"
  },
  "bugs": {
    "url": "https://github.com/MasatoMakino/RAF-ticker/issues"
  },
  "homepage": "https://github.com/MasatoMakino/RAF-ticker#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
